{
  "$schema": "https://ui.shadcn.com/schema.json",
  "style": "default",
  "rsc": true,
  "tsx": true,
  "tailwind": {
    "config": "tailwind.config.ts",
    "css": "src/app/(app)/globals.css",
    "baseColor": "neutral",
    "cssVariables": false,
    "prefix": ""
  },
  "aliases": {
    "components": "@/app/(app)/_components",
    "utils": "@/app/(app)/_helpers",
    "ui": "@/app/(app)/_components/ui",
    "lib": "@/app/(app)/_lib",
    "hooks": "@/app/(app)/_hooks"
  },
  "iconLibrary": "lucide"
}
