    "@types/react-dom": "^19.0.3",
    "eslint": "^9.19.0",
    "eslint-config-next": "15.0.2",
    "postcss": "^8.5.1",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^4.0.0",
    "tailwindcss-animate": "^1.0.7",
    "tailwindcss-intersect": "^2.0.1",
    "tailwindcss-motion": "^1.0.1",
    "typescript": "^5.7.3"
  },
